<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.bhina.bucalcounter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name=".Main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Meal_Menu" >
            <intent-filter>
                <action android:name="android.intent.action.MEAL_MENU" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Calendar" >
            <intent-filter>
                <action android:name="android.intent.action.CALENDAR" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Dining_Halls" >
            <intent-filter>
                <action android:name="android.intent.action.DINING_HALLS" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Enter_Food" >
            <intent-filter>
                <action android:name="android.intent.action.ENTER_FOOD" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Global_Variables" >
            <intent-filter>
                <action android:name="android.intent.action.GLOBAL_VARIABLES" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Daily_Tracker"
            android:label="@string/title_activity_daily__tracker" >
        </activity>
    </application>

</manifest>
